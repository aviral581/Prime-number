
#include <iostream>

using namespace std;

int tracingzero(int num){
    int count=0;
    while(num>=5){                    
        count+=num/5;                          // num is divided by 5 and will get  count last zeros
        num=num/5;                              
    }
    return count;
}
int main()
{
    int n;
    cout<<"Enter the number"<<endl;
    cin>>n;
    int ans=tracingzero(n);
    cout<<"<<---tracing_zeros--->>   "<<ans;

    return 0;
}
