
#include <iostream>

using namespace std;

int main()
{
   int first = 0, second = 1;
   int tmp;
   
   int n;
   cout<<"Enter the value"<<endl;
   cin>>n;
   cout<<first<<second;
   
   
   
   for(int i =2;i<=n-1;i++){
       int tmp=first+second;
       second = tmp;
       first = second;
       cout<< tmp;
   }
   
      
    return 0;
}
